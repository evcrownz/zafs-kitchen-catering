{
    "name": "zafskitchen/catering-system",
    "description": "Zaf's Kitchen Catering Management System",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "google/apiclient": "^2.18.4",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "process-timeout": 600
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
